<Window x:Class="Jishi.StreamToSonos.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="StreamToSonos" Height="81.94" Width="355"
        Icon="Resources/assets_sonos.ico">
    <Grid Margin="0,0,4,0" RenderTransformOrigin="0.505,0.463">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="214*"/>
        </Grid.ColumnDefinitions>
        <ComboBox Name="ZoneList" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="170" Height="21" SelectionChanged="ZoneList_SelectionChanged"/>
        <Button Name="StreamAction" Content="Start Streaming" HorizontalAlignment="Left" Margin="207,9,0,0" VerticalAlignment="Top" Width="114" Grid.ColumnSpan="2" Click="StreamAction_Click"/>
    </Grid>
</Window>
